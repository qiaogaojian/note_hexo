<!DOCTYPE html>
<html>
<%- partial('_partial/header') %>
<body>
<aside class="nav">
    <div class="nav-left">
        <%- fragment_cache('left', function(){
            return partial('_partial/nav-left');
        }) %>
    </div>
    <div class="nav-right">
        <%- fragment_cache('friends', function(){
            return partial('_partial/friends');
        }) %>
        <%- fragment_cache('right', function(){
            return partial('_partial/nav-right');
        }) %>
    </div>
    <div class="hide-list">
        <div class="semicircle" data-title="<%= __('titles.toggle_full_screen') %><%= theme.shortcutKey !== false ? ' ' + __('titles.shortcut_key') + ' s' : '' %>">
            <div class="brackets first"><</div>
            <div class="brackets">&gt;</div>
        </div>
    </div>
</aside>
<div id="post">
    <div class="pjax">
        <%- partial('_partial/article', {post: page, index: false}) %>
    </div>
    <div class="copyright">
        <%- fragment_cache('copyright', function(){
            return partial('_partial/copyright');
        }) %>
    </div>
    <div class="full-toc">
        <%- fragment_cache('toc', function(){
            return partial('_partial/full-toc');
        }) %>
    </div>
</div>
<% if (theme.reward) {%>
<%- fragment_cache('dashang', function(){
    return partial('_partial/dashang');
}) %>
<% } %>
</body>
<script>
var _hmt = _hmt || [];
(function() {
  var hm = document.createElement("script");
  hm.src = "https://hm.baidu.com/hm.js?07b17699976e6c59a5432b46de225325";
  var s = document.getElementsByTagName("script")[0]; 
  s.parentNode.insertBefore(hm, s);
})();
</script>
<%- fragment_cache('footer', function(){
    return partial('_partial/footer');
}) %>
